{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "bundledPackages": [],
  "apiReport": {
    "enabled": false
  },

  "dtsRollup": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-missing-release-tag": {
        "logLevel": "none",
        "addToApiReportFile": false
      }
    }
  },
  "mainEntryPointFilePath": "../../../yfiles-layout-reactflow/dist/index.d.ts",
  "projectFolder": "../../../yfiles-layout-reactflow",
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "../../build/yfiles-layout-reactflow-api.json"
  },
  "compiler": {
    "tsconfigFilePath": "../../../yfiles-layout-reactflow/tsconfig.json"
  }
}
